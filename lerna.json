{
	"npmClient": "npm",
	"packages": ["dictionaries/*", "docs/*"],
	"useNx": false,
	"useWorkspaces": true,
	"version": "independent",
	"command": {
		"publish": {
			"conventionalCommits": true,
			"ignoreChanges": ["*.md", "package-lock.json", "cspell.json"]
		}
	},
	"changelogPreset": {
		"name": "conventionalcommits",
		"issueUrlFormat": "{{host}}/{{owner}}/{{repository}}/issues/{{id}}"
	}
}
